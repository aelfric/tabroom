<%args>
	$round
</%args>
<%init>

	Tab::Panel->set_sql(remove_rooms => "
		update panel
		set room = 0
		where round = ?
	");

	Tab::Panel->sql_remove_rooms->execute($round->id);

	$m->comp("/funclib/hybrid_panels.mas", round => $round, dump => 'yasskween');
	$round->setting('disaster_checked', 0);

	return;

</%init>
