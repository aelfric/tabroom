									$entries{$a}->{'seed'} <=> $entries{$b}->{'seed'}
								} keys %entries
							) {

								next unless $key && $key > -1;
								next unless $entries{$key}{'placement'} == $this_bracket;
								next if $entries{$key}{'opponent'};
</%perl>

								<tr
									id        = "unpaired_<% $key %>"
									class     = "unpaired unpaired_<% $entries{$key}{"placement"} %> entryrows"
									bracket   = "<% $entries{$key}{"placement"} %>"
									entrycode = "<% $entries{$key}{"code"} %>"
									entrywins = "<% $entries{$key}{"wins"} %>"
									side      = "<% $entries{$key}{'sidedue'} %>"
								>

									<td
										class="leftalign entrycodes"
										title = "<% $entries{$key}{'code'} %>"
									>
										<% $entries{$key}{'code'} %>
									</td>

									<td class="centeralign padno">
										<% $entries{$key}{'wins'} %>
									</td>

									<td>
										<% $entries{$key}{'seed'} %>
									</td>

%									if ($powermatch_method eq "sop") {
										<td>
											<% sprintf("%.1f", $entries{$key}{'SOP'}) %>
										</td>
%									}
									<td>
										<% sprintf("%.1f", $entries{$key}{'next_tb'}) %>
									</td>

									<td>
										<% $entries{$key}{'seed_nowins'} %>
									</td>

									<td>
										<% sprintf("%.1f", $entries{$key}{'opp_seed'}) %>
									</td>

									<td class="centeralign">
%										if ($event_settings{bracket_by_ballots}) {
											<% sprintf("%.1f", $entries{$key}{'opp_ballots'}) %>
%										} else {
											<% sprintf("%.1f", $entries{$key}{'opp_wins'}) %>
%										}
									</td>


									<td class="centeralign"
										title="<% $entries{$key}{"aff_count"}.substr($aff_string, 0, 1) %> <% $entries{$key}{"neg_count"}.substr($neg_string, 0, 1) %>">
										<% $entries{$key}{"aff_count"} %>-<% $entries{$key}{"neg_count"} %>
									</td>

									<td class="centeralign" title="Side Last Round">
										<% $entries{$key}{"last_side"} == 1 ? substr($aff_string, 0, 1) : "" %>
										<% $entries{$key}{"last_side"} == 2 ? substr($neg_string, 0, 1) : "" %>
									</td>

									<td class="centeralign">
										<% $entries{$key}{'pulled_up'} %>/<% $entries{$key}{'got_pullup'} %>
									</td>

									<td class="nospace centeralign nowrap actions_<% $key %>">
%										my $warn = had_bye_already($key);
										<span class='half'>
											<a
												class   = "buttonwhite greentext fa fa-sm fa-hand-paper-o thinner"
												title   = "Give this entry a bye"
												onclick = "giveBye(
													<% $key %>,
													'<% $warn %>',
													<% $entries{$key}{'wins'} %>
												);"
											></a>
										</span>
										<span class='half'>
											<a
												class   = "buttonwhite bluetext fa fa-sm fa-arrow-up thinner"
												title   = "Pull entry up into the current bracket"
												onclick = "pullMeUpScotty('UP', <% $key %>);"
											></a>
										</span>
									</td>

								</tr>

%							}	# end team loop

						</tbody>
					</table>
				</div>
%	 		}

		<h5>Current Pairings</h5>

		<&
			"/funclib/tablesorter.mas",
			table     => "pairings",
			nobuttons => 1
		&>

		<table id="pairings">

			<thead>
				<tr class="yellowrow">
					<th class="smaller">Bracket</th>
					<th class="smaller"><% $aff_string %></th>
					<th class="smaller"><% $neg_string %></th>
					<th class="smaller nosort" colspan="2">Action</th>
				</tr>
			</thead>

			<tbody class="smallish">
<%perl>

				my $dbh = Tab::DBI->db_Main();

				my $sth = $dbh->prepare("
					select
						panel.id, panel.letter, panel.bracket, panel.bye,
						aff.id aff, aff.code affcode, aff.name affname,
						neg.id neg, neg.code negcode, neg.name negname

					from panel
						left join ballot affballot on affballot.panel = panel.id and affballot.side = 1
						left join entry aff on aff.id = affballot.entry
						left join ballot negballot on negballot.panel = panel.id and negballot.side = 2
						left join entry neg on neg.id = negballot.entry
					where panel.round = ?
					group by panel.id
				");

				$sth->execute($round_id);
				my $panels = $sth->fetchall_hash();

				foreach my $panel (@{$panels}) {

					my $aff = $panel->{aff};
					my $neg = $panel->{neg};
</%perl>
					<tr
						id    = "pairing_<% $panel->{id} %>"
						title = "Section <% $panel->{letter} %>"
					>

						<td>
							<% $panel->{bracket} || 0 %>
						</td>

						<td>
							<span class="twothirds nowrap leftalign" title="<% $panel->{affname} %>">
								<% $panel->{affcode} %>
							</span>

							<span class="third <% $entries{$aff}{"wins"} ne $panel->{bracket} ? "semibold bluetext" : "" %>">
								<% $entries{$aff}{'wins'} %> <% $win_long_tag %>
							</span>
						</td>

						<td>
%							if ($panel->{bye}) {
								<span class="full redtext semibold marno">
									<% $panel->{bye} %>
								</span>
%							} elsif ($entries{$neg}) {
								<span class="twothirds nowrap leftalign" title="<% $panel->{negname} %>">
									<% $panel->{negcode} %>
								</span>
								<span class="third <% $entries{$neg}{"wins"} ne $panel->{bracket} ? "semibold bluetext" : "" %>">
									<% $entries{$neg}{'wins'} %> <% $win_long_tag %>
								</span>
%							}
						</td>

						<td class="centeralign nospace">
							<a
								class   = "buttonwhite redtext fa fa-trash fa-sm"
								onclick = "deletePanel(
									<% $panel->{id} %>,
									<% $aff %>,
									<% $neg %>
								);"
							></a>
						</td>
					</tr>
%				}
			</tbody>
			</table>
%		}

