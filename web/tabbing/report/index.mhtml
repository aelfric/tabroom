<%args>
	$tourn
	$tourn_settings
	$person
	$person_settings
	$perms         => undef
	$only_category => undef
</%args>
<%init>

	my $dbh = Tab::DBI->db_Main();

	my $checker++
		if ${$perms}{"checker"};

	$checker++ if ${$perms}{"limited"}
		&! ${$perms}{"registration"};

	my @all_timeslots = $tourn->timeslots;

	my ($eventref, $catref) = $m->comp(
		"/funclib/allowed_events.mas",
		tourn => $tourn,
		perms => $perms,
		type  => "admin"
	);

	my @events = @{$eventref};
	my $event = $events[0] if scalar @events == 1;
	my @categories = @{$catref};

	my %types;

	foreach my $oevent (@events) {
		$types{$oevent->type}++;
	}

	my @round_robins;

	foreach my $event (@events) {
		push @round_robins, $event if $event->setting('round_robin');
	}

	my @debate;
	my @speech;
	my @congress;

	@events = sort {$a->name cmp $b->name} @events;

	foreach my $event (@events) {
		push @debate, $event if $event->type ne"speech" && $event->type ne"congress";
		push @speech, $event if $event->type eq "speech";
		push @congress, $event if $event->type eq "congress";
	}

	my %ok_events = map {$_->id => $_} @events;

	my $sth = $dbh->prepare("
		select event.id, round.id, round.name, round.label
			from event, round
		where event.tourn = ?
			and event.id = round.event
		order by event.id, round.name
	");

	$sth->execute($tourn->id);

	my %rounds;

	while (
		my ($event_id, $round_id, $round_name, $round_label) = $sth->fetchrow_array()
	) {

		next unless $ok_events{$event_id};

		$rounds{$event_id}{$round_id}{"number"} = $round_name;

		if ($round_label) {
			$rounds{$event_id}{$round_id}{"name"} = $round_label;
		} else {
			$rounds{$event_id}{$round_id}{"name"} = "Round ".$round_name;
		}
	}

</%init>

